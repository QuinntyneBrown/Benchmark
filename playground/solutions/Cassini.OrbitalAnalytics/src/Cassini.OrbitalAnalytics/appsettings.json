{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Telemetry": {
    "MessageRateHz": 10,
    "DelayThresholdMs": 2000,
    "CacheSizeLimit": 10000,
    "CacheAbsoluteExpirationSeconds": 300,
    "CacheSlidingExpirationSeconds": 60,
    "RateLimitPermitsPerSecond": 1000,
    "RateLimitQueueLimit": 500,
    "BatchSize": 25
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5008"
      }
    }
  }
}
